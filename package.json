{
  "name": "ecclesia",
  "version": "2.0.0",
  "description": "Framework for political and electoral simulations",
  "type": "module",
  "exports": {
    "./election": {
      "import": {
        "types": "./dist/election.d.ts",
        "default": "./dist/election.js"
      },
      "require": {
        "types": "./dist/election.d.cts",
        "default": "./dist/election.cjs"
      }
    },
    "./election/*": {
      "import": {
        "types": "./dist/election/*.d.ts",
        "default": "./dist/election/*.js"
      },
      "require": {
        "types": "./dist/election/*.d.cts",
        "default": "./dist/election/*.cjs"
      }
    },
    "./actors/*": {
      "import": {
        "types": "./dist/actors/*.d.ts",
        "default": "./dist/actors/*.js"
      },
      "require": {
        "types": "./dist/actors/*.d.cts",
        "default": "./dist/actors/*.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gouvernathor/Ecclesia-TS.git"
  },
  "keywords": [
    "election",
    "politics",
    "parliament",
    "simulation"
  ],
  "author": "Gouvernathor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gouvernathor/Ecclesia-TS/issues"
  },
  "homepage": "https://github.com/Gouvernathor/Ecclesia-TS#readme",
  "funding": "https://ko-fi.com/gouvernathor",
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "*"
  },
  "dependencies": {
    "@gouvernathor/python": "^1.0.3",
    "@gouvernathor/rng": "^1.2.1"
  }
}
